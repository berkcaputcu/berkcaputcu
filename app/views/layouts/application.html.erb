<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title><%= content_for?(:title) ? yield(:title) : "Berk Caputcu" %></title>
	<meta name="description" content="">
	<meta name="author" content="">
	<%= stylesheet_link_tag "application", :media => "all" %>
	<%= javascript_include_tag "application" %>
	<%= csrf_meta_tags %>
	<%= yield(:head) %>
</head>
<body>
	<div class="navbar navbar-fixed-top">
		<div class="navbar-inner">
			<div class="container">
				<ul class="nav">
					<li><%= link_to "Berk Caputcu", :root, class: "brand" %></li>
				</ul>
			</div><!--/.nav-collapse -->
		</div>
	</div>
	<div id="main" role="main">
		<div class="container">
			<div class="content">
				<div class="row">
					<div class="span12">
						<%= yield %>
						<br>
						<br>
						<br>
						<br>
					</div>
				</div>
			</div>
		</div> <!--! end of .container -->
	</div> <!--! end of #main -->
</body>
</html>